<%- include("partials/header"); -%>
    <h1>Contact</h1>

    <form action="/contact" method="post">
  <div class="form-group">
    <input  class="form-control" type="text" name="postName" placeholder="Enter your name">
  </div>
  <div class="form-group">
    <input class="form-control" type="email" name="postEmail" placeholder="Enter your email">
  </div>
  <div class="form-group">
    <input class="form-control" type="Phone" name="postPhone" placeholder="Enter your phone">
  </div>
  <div class="form-group">
    <textarea  class="form-control" name="postMessage" rows="5" cols="40" placeholder="Type a message...."></textarea>
  </div>
  <div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </div>
</form>

<%- include("partials/footer"); -%>
